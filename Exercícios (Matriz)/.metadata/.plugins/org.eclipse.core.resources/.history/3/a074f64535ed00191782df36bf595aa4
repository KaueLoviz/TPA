public class A {
	public static void main(String[]args) {
		int v[][]=new int[4][4];
		int i, j;
		for(i=0; i<=3; i++) {
			for(j=0; j<=3; j++) {
				v[i][j]=2;
			}
		}
		for(i=0; i<=3; i++) {
			for(j=0; j<=3; j++) {
				System.out.print("[ "+v[(i-1)*2][j]+" ]");
				if(j==3) {
					System.out.print("\n");
				}
			}
		}
	}
}