public class B {
	public static void main(String[]args) {
		int m[][]=new int[3][3];
		int i, j, ma=m[0][0], me=m[0][0], l=0, c=0, ml=0, mc=0;
		for(i=0; i<=2; i++) {
			for(j=0; j<=2; j++) {
				m[i][j]=(int)(Math.random()*30);
				if(m[i][j]<me) {
					me=m[i][j];
					ml=i;
					mc=j;
				}
				if(m[i][j]>ma) {
					ma=m[i][j];
					l=i;
					c=j;
				}
			}
		}
		for(i=0; i<=2; i++) {
			for(j=0; j<=2; j++) {
				System.out.print("[ "+m[i][j]+" ]");
				if(j==2) {
					System.out.print("\n");
				}
			}
		}
		System.out.println("O maior número foi: "+ma);
		System.out.println("Linha: "+l);
		System.out.println("Coluna: "+c);
		System.out.println("-------------------------");
		System.out.println("O menor número foi: "+me);
		System.out.println("Linha: "+ml);
		System.out.println("Coluna: "+mc);

	}
}